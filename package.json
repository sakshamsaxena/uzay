{
  "name": "uzay",
  "version": "0.0.1",
  "description": "Uzay is a Node.js and MongoDB based modern blog engine.",
  "main": "index.js",
  "scripts": {
    "docs": "npx apidoc -i ./routes -o ./docs",
    "postinstall": "node config/copyConfig.js",
    "precommit": "npm test",
    "start": "npx nodemon index.js",
    "test": "npx standard && npx mocha --full-trace"
  },
  "author": "Saksham Saxena",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakshamsaxena/uzay.git"
  },
  "apidoc": {
    "title": "Uzay",
    "order": [
      "SignUp",
      "VerifyUser",
      "GetBlogPostByID",
      "GetBlogPostsByTagName",
      "GetCommentByID",
      "PostCommentToBlogPost",
      "PatchLikeOnBlogPost",
      "PatchDislikeOnBlogPost",
      "PatchLikeOnBlogPostComment",
      "PatchDislikeOnBlogPostComment",
      "GetUserInfo",
      "GetAllLikedContentByUser",
      "GetAllDislikedContentByUser",
      "GetAllPostsByUser",
      "GetAllCommentsByUser",
      "PostNewPostByUser"
    ]
  },
  "standard": {
    "ignore": [
      "docs"
    ]
  },
  "dependencies": {
    "bcrypt": "3.0.0",
    "body-parser": "1.18.3",
    "express": "4.16.3",
    "jsonwebtoken": "8.3.0",
    "mongoose": "5.2.7",
    "mongoose-plugin-autoinc": "1.1.9",
    "morgan": "^1.9.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "nodemon": "1.18.3",
    "should": "13.1.3",
    "standard": "11.0.0",
    "supertest": "3.0.0"
  }
}
